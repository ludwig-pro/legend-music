        "expo-navigation-bar",
        "expo-router",
        "expo-splash-screen",
        "expo-sqlite",
        "expo-status-bar",
        "expo-system-ui"
      ]
    }
  },
  "trustedDependencies": [
    "@shopify/react-native-skia"
  ],
  "resolutions": {}
